<h1>Allllllll the Articles!</h1>
<h2><%= link_to 'Write something!', new_article_path %></h2>

<ul>
  <% @articles.each do |article| %>
  <li><%= article.title %> | <%= article.text %> | <%= link_to 'Show', article_path(article)%> | <%= link_to 'Edit', edit_article_path(article) %></li>
  <% end %>
</ul>
